{
    "name": "tome",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "postinstall": "prisma generate",
        "dev": "next dev",
        "build": "prisma generate && next build",
        "start": "next start",
        "wip": "bun run fmt:write && git add . && git commit -m \"WIP\" && git push",
        "type-check": "tsc --noEmit",
        "lint": "eslint",
        "fmt:write": "prettier --write \"**/*.{ts,tsx,mdx,json}\" --cache --experimental-ternaries",
        "fmt:check": "prettier --check \"**/*.{ts,tsx,mdx,json}\" --cache",
        "db:generate": "prisma generate",
        "db:push": "prisma db push",
        "db:studio": "prisma studio",
        "db:seed": "prisma db seed",
        "pulumi:login": "cd aws && pulumi login s3://tome-pulumi-state-2921",
        "deploy:dev": "bun run pulumi:login && cd aws && pulumi up --stack dev -y",
        "destroy:dev": "bun run pulumi:login && cd aws && pulumi down --stack dev -y"
    },
